{
  "name": "lyric-get-electron",
  "productName": "Lyric Get",
  "version": "2024.03.02",
  "description": "Desktop client to Lyric Get service",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franklai/lyric-get-electron.git"
  },
  "author": "franklai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/franklai/lyric-get-electron/issues"
  },
  "homepage": "https://github.com/franklai/lyric-get-electron/",
  "devDependencies": {
    "electron": "^29.1.0",
    "electron-builder": "^24.12.0"
  },
  "build": {
    "appId": "com.blogspot.franklai-lyric-get",
    "win": {
      "target": "nsis"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": "pkg"
    }
  },
  "dependencies": {
    "lyric-get-js": "https://github.com/franklai/lyric-get-js"
  }
}
